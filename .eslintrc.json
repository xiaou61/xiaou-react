{
  "env": {
    "browser": true,
    "es2021": true
  },
  //这个是规则的集合
  "extends": [
    "eslint:recommended",
    "pretter",
    "plugin:@typescript-eslint/recommended"
  ],
  //这个是一个解析器
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  //规则的合集
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error"
  }
}
